{
    "terminal.integrated.defaultProfile.windows": "MSYS",
    "terminal.integrated.profiles.windows": {
        "MSYS": {
            "path": "C:\\msys64\\usr\\bin\\bash.exe",
            "args": [
                "--login",
                "-i"
            ],
            "icon": "terminal-bash",
            "env": {
                "MSYSTEM": "UCRT64",
                "CHERE_INVOKING": "1"
            }
        },
        "PowerShell": {
            "source": "PowerShell",
            "icon": "terminal-powershell"
        },
        "Command Prompt": {
            "path": [
                "${env:windir}\\Sysnative\\cmd.exe",
                "${env:windir}\\System32\\cmd.exe"
            ],
            "args": [],
            "icon": "terminal-cmd"
        },
        "Git Bash": {
            "source": "Git Bash"
        },
        "WSL": {
            "path": "wsl.exe",
            "args": ["--login"],
            "icon": "terminal-bash"
        }
    }
}

